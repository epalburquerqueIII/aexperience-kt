<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View"/>

        <variable name="addusuarioviewmodel" type="com.epalburquerqueiii.aexperience.Data.Model.Usuario"/>


    </data>

<androidx.constraintlayout.widget.ConstraintLayout

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.Usuarios.UsuarioActivity">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="387dp"
            android:layout_height="61dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="12dp"
            tools:layout_editor_absoluteY="577dp">

            <EditText
                android:id="@+id/Newsletter"
                android:layout_width="127dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Newsletter" />

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <Switch
                android:id="@+id/opcionesNl"
                android:layout_width="111dp"
                android:layout_height="41dp"
                android:text="No" />

        </LinearLayout>

        <include
            layout="@layout/editupdate_botton"
            android:layout_width="403dp"
            android:layout_height="128dp"
            tools:layout_editor_absoluteX="3dp"
            tools:layout_editor_absoluteY="654dp" />

        <EditText
            android:id="@+id/FechaBaja"
            style="@style/Widget.MaterialComponents.TextInputEditText.OutlinedBox.Dense"
            android:layout_width="393dp"
            android:layout_height="52dp"

            android:background="@drawable/background_rectangle_black"
            android:drawablePadding="8dp"
            android:hint="Fecha de Baja"
            android:imeActionId="6"
            android:imeOptions="actionUnspecified"
            android:maxLines="1"
            android:paddingEnd="20dp"
            android:singleLine="true"
            android:text="@{addusuarioviewmodel.FechaBaja.toString()}"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="22sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="508dp" />

        <EditText
            android:id="@+id/SesionesBonos"
            style="@style/Widget.MaterialComponents.TextInputEditText.OutlinedBox.Dense"
            android:layout_width="387dp"
            android:layout_height="48dp"

            android:background="@drawable/background_rectangle_black"
            android:drawablePadding="8dp"
            android:hint="Sesiones Bonos"
            android:imeActionId="6"
            android:imeOptions="actionUnspecified"
            android:maxLines="1"
            android:paddingEnd="20dp"
            android:singleLine="true"
            android:text="@{addusuarioviewmodel.SesionesBonos.toString()}"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="22sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="450dp" />

        <EditText
            android:id="@+id/Telefono"
            style="@style/Widget.MaterialComponents.TextInputEditText.OutlinedBox.Dense"
            android:layout_width="386dp"
            android:layout_height="53dp"

            android:background="@drawable/background_rectangle_black"
            android:drawablePadding="8dp"
            android:hint="Telefono"
            android:imeActionId="6"
            android:imeOptions="actionUnspecified"
            android:maxLines="1"
            android:paddingEnd="20dp"
            android:singleLine="true"
            android:text="@{addusuarioviewmodel.Telefono}"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="22sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="381dp" />

        <EditText
            android:id="@+id/Tipo"
            style="@style/Widget.MaterialComponents.TextInputEditText.OutlinedBox.Dense"
            android:layout_width="388dp"
            android:layout_height="48dp"

            android:background="@drawable/background_rectangle_black"
            android:drawablePadding="8dp"
            android:hint="Tipo"
            android:imeActionId="6"
            android:imeOptions="actionUnspecified"
            android:maxLines="1"
            android:paddingEnd="20dp"
            android:singleLine="true"
            android:text="@{addusuarioviewmodel.Tipo.toString()}"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="22sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="318dp" />

        <EditText
            android:id="@+id/Email"
            style="@style/Widget.MaterialComponents.TextInputEditText.OutlinedBox.Dense"
            android:layout_width="382dp"
            android:layout_height="53dp"

            android:background="@drawable/background_rectangle_black"
            android:drawablePadding="8dp"
            android:hint="Email"
            android:imeActionId="6"
            android:imeOptions="actionUnspecified"
            android:maxLines="1"
            android:paddingEnd="20dp"
            android:singleLine="true"
            android:text="@{addusuarioviewmodel.Email}"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="22sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="250dp" />

        <EditText
            android:id="@+id/Nif"
            style="@style/Widget.MaterialComponents.TextInputEditText.OutlinedBox.Dense"
            android:layout_width="385dp"
            android:layout_height="50dp"

            android:background="@drawable/background_rectangle_black"
            android:drawablePadding="8dp"
            android:hint="N.I.F."
            android:imeActionId="6"
            android:imeOptions="actionUnspecified"
            android:maxLines="1"
            android:paddingEnd="20dp"
            android:singleLine="true"
            android:text="@{addusuarioviewmodel.Nif}"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="22sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="187dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/txt_activity2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Usuario"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/grey_900"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.538"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Nombre"
        style="@style/Widget.MaterialComponents.TextInputEditText.OutlinedBox.Dense"
        android:layout_width="381dp"
        android:layout_height="50dp"
        android:layout_marginStart="15dp"
        android:background="@drawable/background_rectangle_black"
        android:drawablePadding="8dp"
        android:hint="Nombre"
        android:imeActionId="6"
        android:imeOptions="actionUnspecified"
        android:maxLines="1"
        android:paddingEnd="20dp"
        android:singleLine="true"
        android:text="@{addusuarioviewmodel.Nombre}"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/txt_activity2"
        app:layout_constraintVertical_bias="0.125"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>
</layout>